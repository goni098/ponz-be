datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id      BigInt @id @default(autoincrement())
  address String @unique @db.VarChar
}

model renew_token {
  id      BigInt @id @default(autoincrement())
  user_id BigInt @unique
  token   String @db.VarChar
}
